{
	"manifest_version": 2,
	"name": "You Guys to You Babies",
	"version": "1.0",
	"description": "Replaces the text 'you guys' with 'you babies.'",
	"icons":  { "128": "icon.png" },
	"browser_action": {
		"default_icon": "icon.png"
	},
	"content_scripts": 
	[
		{
			"matches": ["*://*/*"],
			"js": ["content_script.js"],
			"run_at": "document_end"
		}
	]
}

